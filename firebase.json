{
  "hosting": {
    "public": "out", 
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "run": {
          "serviceId": "accounting-api",
          "region": "asia-northeast1" 
        }
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}